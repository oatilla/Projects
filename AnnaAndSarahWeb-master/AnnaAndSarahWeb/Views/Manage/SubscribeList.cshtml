@model IEnumerable<AnnaAndSarahWeb.Models.tbl_SubscribeList>

@{
    ViewBag.Title = "SubscribeList";
    Layout = "~/Views/Shared/Admin.cshtml";
}


<h2>Subscribe List</h2>
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-body">
                <table id="example2" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>Mail Adress</th>
                            <th>Tarih</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MailAddress)
                                </td>
                                <td style="width: 160px;">
                                    @Html.DisplayFor(modelItem => item.Tarih)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div><!-- /.box-body -->
        </div>
    </div><!-- /.col -->
</div>